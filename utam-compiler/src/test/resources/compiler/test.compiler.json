{
  "module": "myModule",
  "pageObjectsRootDir": "/src/test/resources/compiler",
  "pageObjectsFilesMask": "(.*)\\.utam\\.json$",
  "pageObjectsOutputDir": "/src/test/java",
  "resourcesOutputDir": "/src/test/resources",
  "namespaces": [
    {
      "typeMatch": "utam-profiles",
      "pathMatch": ".*/spec"
    }
  ],
  "profiles": [
    {
      "name": "platform",
      "values": [
        "ios",
        "ios_phone",
        "ios_tablet",
        "android",
        "android_phone",
        "android_tablet"
      ]
    }
  ],
  "lint" : {
    "throwError" : false
  }
}
