{
  "name": "myModule",
  "pageObjectsDirectory": "/src/test/resources/spec",
  "pageObjectsFilesMask": "(.*)\\.utam\\.json$",
  "namespaces" : [
    {
      "typePrefix" : "utam-one",
      "pathMatchRegex": ".*/one"
    },
    {
      "typePrefix" : "utam-two",
      "pathMatchRegex": ".*/two"
    }
  ],
  "target": {
    "pageObjects": "/src/test/java/pageObjects",
    "resources": "/src/test/resources",
    "unitTests": "/src/test/java",
    "unitTestsRunner": "JUNIT"
  },
  "profiles": [
    {
      "name": "platform",
      "values": [
        "web",
        "ios",
        "android"
      ]
    }
  ]
}